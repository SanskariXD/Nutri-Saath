{
  "name": "label-backend",
  "version": "1.0.0",
  "description": "Clean Express + TypeScript API for Nutri Saath",
  "main": "dist/server.js",
  "_moduleAliases": {
  "@": "dist"
  },
  "scripts": {
    "build": "tsc && tsc-alias",
    "start": "node -r module-alias/register dist/server.js",
    "dev": "ts-node-dev --respawn -r tsconfig-paths/register src/index.ts",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "express",
    "nutrisaath"
  ],
  "author": "Nutri Saath Team",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "dependencies": {
    "@sentry/node": "^7.116.0",
    "axios": "^1.7.7",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-rate-limit": "^8.1.0",
    "google-auth-library": "^9.14.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "mongoose": "^7.8.1",
    "morgan": "^1.10.0",
    "multer": "^2.0.0",
    "rate-limiter-flexible": "^2.4.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.12",
    "eslint": "^8.57.0",
    "ts-node-dev": "^2.0.0"
  }
}
